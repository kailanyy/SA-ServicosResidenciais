CREATE DATABASE application_database
    WITH
    OWNER = postgres
    ENCODING = 'UTF8'
    CONNECTION LIMIT = -1;

CREATE TABLE person(
    personId SERIAL PRIMARY KEY,
    email VARCHAR(45) NOT NULL,
    pass VARCHAR(45) NOT NULL,
    fullName VARCHAR(45) NOT NULL,
    phoneNumber VARCHAR(45) NOT NULL,
    about VARCHAR(100) NOT NULL
);

